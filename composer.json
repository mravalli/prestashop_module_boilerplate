{
  "name": "mravalli/prestashop_module_boilerplate",
  "description": "Simple Boilerplate to develop a module for Prestashop",
  "minimum-stability": "stable",
  "license": "AFL-3.0",
  "authors": [
    {
      "name": "Mario Ravalli",
      "email": "mario@raval.li"
    }
  ],
  "require": {
  },
  "require-dev": {
    "phpstan/phpstan": "1.10.5",
    "phpunit/phpunit": "9.6.4",
    "prestashop/php-dev-tools": "v4.3.0"
  },
  "autoload": {
    "psr-4": {
      "MyModule\\": "src/"
    },
    "classmap": []
  },
  "config": {
    "platform": {
      "php": "7.4"
    },
    "preferred-install": "dist",
    "prepend-autoloader": false
  },
  "type": "prestashop-module"
}